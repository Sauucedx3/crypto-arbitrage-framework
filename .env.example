# Polygon RPC URL (required for flash loans and gasless transactions)
MAINNET_RPC_URL=https://polygon-rpc.com

# Biconomy API key (required for gasless transactions)
BICONOMY_API_KEY=your-biconomy-api-key

# Private key for signing transactions (required for both flash loans and gasless transactions)
PRIVATE_KEY=your-private-key-without-0x-prefix

# Polygonscan API key (optional, used for fetching contract ABIs)
POLYGONSCAN_API_KEY=your-polygonscan-api-key

# Coinmarketcap API key (required for the original arbitrage framework)
COINMARKETCAP_API_KEY=your-coinmarketcap-api-key